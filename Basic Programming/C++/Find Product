/*
You have been given an array A of size N consisting of positive integers. 
You need to find and print the product of all the number in this array Modulo 10^9+7 .
*/


#include <iostream>
#include <cmath>
using namespace std;
int main()
{
    int sizeofarray;
    cin >> sizeofarray;
    int a[sizeofarray];
    long long int p=1;
    for (int i = 0; i < sizeofarray; ++i)
    {
    cin >> a[i];
    p=(p*a[i])%(1000000007);
    }
    cout<<p;
    return(0);
}
